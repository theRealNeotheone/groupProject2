

<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>Coding Bootcamp Cheat Sheet</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">Ask Anything Coding Bootcamp Related!</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="example-text" placeholder="Enter a bootcamp question">
          </div>
          <div class="form-group">
            <label for="example-description">Coding Snippets</label>
            <textarea class="form-control" id="example-description" aria-describedby="example-description" placeholder="Enter a bootcamp description"></textarea>
          </div>
          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <h3>Results:</h3>
        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="sequelize/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100" src="../public/assets/images/ucdbootcamp.png" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="../images/code.jpg" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="../images/computerroom.jpg" alt="Third slide">
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="../public/assets/js/index.js"></script>
