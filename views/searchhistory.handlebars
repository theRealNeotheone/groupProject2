<div id="searchhistory">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">

<div class="card">
          <div class="card-header">
            <h3 style="color: black;">Search History:</h3>
          </div>
          <div class="card-body">
            <ul id="glossaryList" class="list-group"></ul>
          </div>
</div>

        <ul id="searchhistory-list" class="list-group">
          {{#each searchhistory}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="sequelize/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

